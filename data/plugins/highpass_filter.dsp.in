import ("stdfaust.lib");
import ("zrythm-utils.lib");

hp_filter = fi.highpass (2, filter_freq)
  with {
    filter_freq = hslider ("Frequency [unit:Hz] [scale:log] [tooltip:Transition frequency]", 200, 10, 18000, 1) : si.smoo;
  };

process = par (i, 2, hp_filter);
