@prefix doap:  <http://usefulinc.com/ns/doap#> .
@prefix foaf:  <http://xmlns.com/foaf/0.1/> .
@prefix lv2:   <http://lv2plug.in/ns/lv2core#> .
@prefix ui:    <http://lv2plug.in/ns/extensions/ui#> .
@prefix epp:   <http://lv2plug.in/ns/ext/port-props#> .
@prefix atom:  <http://lv2plug.in/ns/ext/atom#> .
@prefix rdf:   <http://www.w3.org/1999/02/22-rdf-syntax-ns#> .
@prefix rdfs:  <http://www.w3.org/2000/01/rdf-schema#> .
@prefix units: <http://lv2plug.in/ns/extensions/units#> .
@prefix urid:  <http://lv2plug.in/ns/ext/urid#> .
<https://lv2.zrythm.org/faust-builtin/triple_synth>
       a lv2:Plugin, lv2:InstrumentPlugin ;
       doap:name "Triple Synth" ;
       lv2:binary <triple_synth@LIB_SUFFIX@> ;
       lv2:requiredFeature urid:map ;
       lv2:optionalFeature epp:supportsStrictBounds ;
       lv2:optionalFeature lv2:hardRTCapable ;
       doap:maintainer [ foaf:name "Zrythm DAW" ] ;
       rdfs:comment "Synth with 3 detuned oscillators" ;
       doap:revision "1.0" ;
       doap:license "AGPL-3.0-or-later" ;
    lv2:port [
	a lv2:InputPort ;
	a lv2:ControlPort ;
	lv2:index 0 ;
	lv2:symbol "Pitchwheel_0" ;
	lv2:name "Pitchwheel" ;
        lv2:portProperty epp:hasStrictBounds ;
        epp:rangeSteps 3999 ;
	lv2:default 0 ;
	lv2:minimum -2 ;
	lv2:maximum 2 ;
    ] , [
	a lv2:InputPort ;
	a lv2:ControlPort ;
	lv2:index 1 ;
	lv2:symbol "Waveform_1" ;
	lv2:name "Waveform" ;
        lv2:portProperty epp:hasStrictBounds ;
        epp:rangeSteps 3 ;
	lv2:default 3 ;
	lv2:minimum 0 ;
	lv2:maximum 3 ;
    ] , [
	a lv2:InputPort ;
	a lv2:ControlPort ;
	lv2:index 2 ;
	lv2:symbol "Attack_2" ;
	lv2:name "Attack" ;
        lv2:portProperty epp:hasStrictBounds ;
        epp:rangeSteps 1998 ;
	lv2:default 0.03 ;
	lv2:minimum 0.001 ;
	lv2:maximum 2 ;
    ] , [
	a lv2:InputPort ;
	a lv2:ControlPort ;
	lv2:index 3 ;
	lv2:symbol "Decay_3" ;
	lv2:name "Decay" ;
        lv2:portProperty epp:hasStrictBounds ;
        epp:rangeSteps 3999 ;
	lv2:default 0.3 ;
	lv2:minimum 0.001 ;
	lv2:maximum 4 ;
    ] , [
	a lv2:InputPort ;
	a lv2:ControlPort ;
	lv2:index 4 ;
	lv2:symbol "Sustain_4" ;
	lv2:name "Sustain" ;
        lv2:portProperty epp:hasStrictBounds ;
        epp:rangeSteps 100 ;
	lv2:default 1 ;
	lv2:minimum 0 ;
	lv2:maximum 1 ;
    ] , [
	a lv2:InputPort ;
	a lv2:ControlPort ;
	lv2:index 5 ;
	lv2:symbol "Release_5" ;
	lv2:name "Release" ;
        lv2:portProperty epp:hasStrictBounds ;
        epp:rangeSteps 3999 ;
	lv2:default 0.4 ;
	lv2:minimum 0.001 ;
	lv2:maximum 4 ;
    ] , [
	a lv2:InputPort ;
	a lv2:ControlPort ;
	lv2:index 6 ;
	lv2:symbol "Tremolo_Freq_6" ;
	lv2:name "Tremolo Freq" ;
        lv2:portProperty epp:hasStrictBounds ;
        epp:rangeSteps 1000 ;
	lv2:default 0 ;
	lv2:minimum 0 ;
	lv2:maximum 20 ;
	units:unit [
            a            units:Unit ;
            units:name   "Hz" ;
            units:symbol "Hz" ;
            units:render "%f Hz"
	] ;
    ] , [
	a lv2:InputPort ;
	a lv2:ControlPort ;
	lv2:index 7 ;
	lv2:symbol "Tremolo_Depth_7" ;
	lv2:name "Tremolo Depth" ;
        lv2:portProperty epp:hasStrictBounds ;
        epp:rangeSteps 999 ;
	lv2:default 0 ;
	lv2:minimum 0 ;
	lv2:maximum 1 ;
    ] , [
	a lv2:InputPort ;
	a lv2:ControlPort ;
	lv2:index 8 ;
	lv2:symbol "Cutoff_8" ;
	lv2:name "Cutoff" ;
        lv2:portProperty epp:hasStrictBounds ;
        epp:rangeSteps 1989000 ;
	lv2:default 10000 ;
	lv2:minimum 110 ;
	lv2:maximum 20000 ;
    ] , [
	a lv2:InputPort ;
	a lv2:ControlPort ;
	lv2:index 9 ;
	lv2:symbol "Cutoff_Q_9" ;
	lv2:name "Cutoff Q" ;
        lv2:portProperty epp:hasStrictBounds ;
        epp:rangeSteps 190 ;
	lv2:default 1 ;
	lv2:minimum 1 ;
	lv2:maximum 20 ;
    ] , [
	a lv2:InputPort ;
	a lv2:ControlPort ;
	lv2:index 10 ;
	lv2:symbol "Detune_10" ;
	lv2:name "Detune" ;
        lv2:portProperty epp:hasStrictBounds ;
        epp:rangeSteps 999 ;
	lv2:default 0.1 ;
	lv2:minimum 0 ;
	lv2:maximum 1 ;
    ] , [
	a lv2:InputPort ;
	a lv2:ControlPort ;
	lv2:index 11 ;
	lv2:symbol "Vibrato_Freq_11" ;
	lv2:name "Vibrato Freq" ;
        lv2:portProperty epp:hasStrictBounds ;
        epp:rangeSteps 1000 ;
	lv2:default 0 ;
	lv2:minimum 0 ;
	lv2:maximum 20 ;
	units:unit [
            a            units:Unit ;
            units:name   "Hz" ;
            units:symbol "Hz" ;
            units:render "%f Hz"
	] ;
    ] , [
	a lv2:InputPort ;
	a lv2:ControlPort ;
	lv2:index 12 ;
	lv2:symbol "Vibrato_Depth_12" ;
	lv2:name "Vibrato Depth" ;
        lv2:portProperty epp:hasStrictBounds ;
        epp:rangeSteps 999 ;
	lv2:default 0 ;
	lv2:minimum 0 ;
	lv2:maximum 1 ;
    ] , [
	a lv2:OutputPort ;
	a lv2:AudioPort ;
	lv2:index 13 ;
	lv2:symbol "out0" ;
	lv2:name "out0" ;
    ] , [
	a lv2:OutputPort ;
	a lv2:AudioPort ;
	lv2:index 14 ;
	lv2:symbol "out1" ;
	lv2:name "out1" ;
    ] , [
	a lv2:InputPort ;
	a atom:AtomPort ;
	atom:bufferType atom:Sequence ;
	atom:supports <http://lv2plug.in/ns/ext/midi#MidiEvent> ;
	lv2:index 15 ;
	lv2:symbol "midiin" ;
	lv2:name "midiin"
    ] , [
	a lv2:InputPort ;
	a lv2:ControlPort ;
	lv2:index 16 ;
	lv2:symbol "polyphony" ;
	lv2:name "polyphony" ;
        lv2:portProperty epp:hasStrictBounds ;
#       lv2:portProperty epp:expensive ;
        lv2:portProperty lv2:integer ;
        epp:rangeSteps 11 ;
	lv2:default 12 ;
	lv2:minimum 1 ;
	lv2:maximum 12 ;
	lv2:portProperty <http://lv2plug.in/ns/ext/port-props#notOnGUI>
    ]
.
