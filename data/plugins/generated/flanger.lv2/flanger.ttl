@prefix doap:  <http://usefulinc.com/ns/doap#> .
@prefix foaf:  <http://xmlns.com/foaf/0.1/> .
@prefix lv2:   <http://lv2plug.in/ns/lv2core#> .
@prefix ui:    <http://lv2plug.in/ns/extensions/ui#> .
@prefix epp:   <http://lv2plug.in/ns/ext/port-props#> .
@prefix atom:  <http://lv2plug.in/ns/ext/atom#> .
@prefix rdf:   <http://www.w3.org/1999/02/22-rdf-syntax-ns#> .
@prefix rdfs:  <http://www.w3.org/2000/01/rdf-schema#> .
@prefix units: <http://lv2plug.in/ns/extensions/units#> .
@prefix urid:  <http://lv2plug.in/ns/ext/urid#> .
<https://lv2.zrythm.org/faust-builtin/flanger>
       a lv2:FlangerPlugin, lv2:Plugin ;
       doap:name "Flanger" ;
       lv2:binary <flanger@LIB_SUFFIX@> ;
       lv2:requiredFeature urid:map ;
       lv2:optionalFeature epp:supportsStrictBounds ;
       lv2:optionalFeature lv2:hardRTCapable ;
       doap:maintainer [ foaf:name "Zrythm DAW" ] ;
       rdfs:comment "Flanger effect" ;
       doap:revision "1.0" ;
       doap:license "AGPL-3.0-or-later" ;
    lv2:port [
	a lv2:InputPort ;
	a lv2:ControlPort ;
	lv2:index 0 ;
	lv2:symbol "Invert_Flange_Sum_0" ;
	lv2:name "Invert Flange Sum" ;
        lv2:portProperty epp:hasStrictBounds ;
        lv2:portProperty lv2:toggled ;
	lv2:default 0.00000 ;
	lv2:minimum 0.00000 ;
	lv2:maximum 1.00000 ;
    ] , [
	a lv2:OutputPort ;
	a lv2:ControlPort ;
	lv2:index 1 ;
	lv2:symbol "Flange_LFO_1" ;
	lv2:name "Flange LFO" ;
	lv2:default -1.5 ;
	lv2:minimum -1.5 ;
	lv2:maximum 1.5 ;
	rdfs:comment "Display sum of flange delays" ;
    ] , [
	a lv2:InputPort ;
	a lv2:ControlPort ;
	lv2:index 2 ;
	lv2:symbol "Speed_2" ;
	lv2:name "Speed" ;
        lv2:portProperty epp:hasStrictBounds ;
        epp:rangeSteps 1000 ;
	lv2:default 0.5 ;
	lv2:minimum 0 ;
	lv2:maximum 10 ;
	units:unit [
            a            units:Unit ;
            units:name   "Hz" ;
            units:symbol "Hz" ;
            units:render "%f Hz"
	] ;
    ] , [
	a lv2:InputPort ;
	a lv2:ControlPort ;
	lv2:index 3 ;
	lv2:symbol "Depth_3" ;
	lv2:name "Depth" ;
        lv2:portProperty epp:hasStrictBounds ;
        epp:rangeSteps 999 ;
	lv2:default 1 ;
	lv2:minimum 0 ;
	lv2:maximum 1 ;
    ] , [
	a lv2:InputPort ;
	a lv2:ControlPort ;
	lv2:index 4 ;
	lv2:symbol "Feedback_4" ;
	lv2:name "Feedback" ;
        lv2:portProperty epp:hasStrictBounds ;
        epp:rangeSteps 1997 ;
	lv2:default 0 ;
	lv2:minimum -0.999 ;
	lv2:maximum 0.999 ;
    ] , [
	a lv2:InputPort ;
	a lv2:ControlPort ;
	lv2:index 5 ;
	lv2:symbol "Flange_Delay_5" ;
	lv2:name "Flange Delay" ;
        lv2:portProperty epp:hasStrictBounds ;
        epp:rangeSteps 20000 ;
	lv2:default 10 ;
	lv2:minimum 0 ;
	lv2:maximum 20 ;
	units:unit [
            a            units:Unit ;
            units:name   "ms" ;
            units:symbol "ms" ;
            units:render "%f ms"
	] ;
    ] , [
	a lv2:InputPort ;
	a lv2:ControlPort ;
	lv2:index 6 ;
	lv2:symbol "Delay_Offset_6" ;
	lv2:name "Delay Offset" ;
        lv2:portProperty epp:hasStrictBounds ;
        epp:rangeSteps 20000 ;
	lv2:default 1 ;
	lv2:minimum 0 ;
	lv2:maximum 20 ;
	units:unit [
            a            units:Unit ;
            units:name   "ms" ;
            units:symbol "ms" ;
            units:render "%f ms"
	] ;
    ] , [
	a lv2:InputPort ;
	a lv2:ControlPort ;
	lv2:index 7 ;
	lv2:symbol "Flanger_Output_Level_7" ;
	lv2:name "Flanger Output Level" ;
        lv2:portProperty epp:hasStrictBounds ;
        epp:rangeSteps 700 ;
	lv2:default 0 ;
	lv2:minimum -60 ;
	lv2:maximum 10 ;
	units:unit [
            a            units:Unit ;
            units:name   "dB" ;
            units:symbol "dB" ;
            units:render "%f dB"
	] ;
    ] , [
	a lv2:InputPort ;
	a lv2:AudioPort ;
	lv2:index 8 ;
	lv2:symbol "in0" ;
	lv2:name "in0" ;
    ] , [
	a lv2:InputPort ;
	a lv2:AudioPort ;
	lv2:index 9 ;
	lv2:symbol "in1" ;
	lv2:name "in1" ;
    ] , [
	a lv2:OutputPort ;
	a lv2:AudioPort ;
	lv2:index 10 ;
	lv2:symbol "out0" ;
	lv2:name "out0" ;
    ] , [
	a lv2:OutputPort ;
	a lv2:AudioPort ;
	lv2:index 11 ;
	lv2:symbol "out1" ;
	lv2:name "out1" ;
    ]
.
